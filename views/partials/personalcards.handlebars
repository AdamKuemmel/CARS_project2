<div id="card" style="width: 18rem;">
  {{! <img src="" class="card-img-top" alt=".IMG GOES HERE.." /> }}
  <div class="card-body">
    <img width="90%" src={{img_url}} alt="car pic" />
    <p class="card-text">Make:{{car_make}}</p>
    <p class="card-text">Model:{{car_model}}</p>
    <p class="card-text">Year:{{car_year}}</p>
    <a href="/api/cars/{{id}}" id="view_car" class="cloudinary-button">View Car</a>
    <button id="deletebutton" data-id={{id}} class="cloudinary-button">Delete this Car</button>
    <a
      href="/api/cars/update/{{id}}"
      id="updatebutton"
      class="cloudinary-button"
    >Update your Car</a>

  </div>
</div>

<script src="/js/delete.js"></script>